{
  "id": "nodebb-plugin-image-sizer",
  "name": "Image Sizer",
  "description": "Adds image sizing markup to embedded images",
  "url": "https://github.com/ariastel/nodebb-plugin-image-sizer",
  "library": "./library.js",
  "hooks": [
    {
      "hook": "filter:parse.post",
      "method": "parsePost",
      "priority": 6
    },
    {
      "hook": "filter:parse.raw",
      "method": "parseRaw",
      "priority": 6
    }
  ]
}